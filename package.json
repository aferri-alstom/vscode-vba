{
    "name": "avb",
    "displayName": "Visual Studio Code VBA",
    "description": "VBA language syntax highlighting support and code snippets for VSCode",
    "version": "1.11.0",
    "icon": "images/logo.png",
    "publisher": "aferri",
    "engines": {
        "vscode": "^1.8.1"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aferri-alstom/vscode-vba.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "avb",
                "aliases": [
                    "avb"
                ],
                "extensions": [
                    ".bas",
                    ".cls",
                    ".frm",
                    ".sym"
                ],
                "configuration": "./avb.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "avb",
                "scopeName": "source.vba",
                "path": "./syntaxes/avb.json"
            }
        ],
        "snippets": [
            {
                "language": "avb",
                "path": "./snippets/avb.json"
            }
        ]
    }
}
